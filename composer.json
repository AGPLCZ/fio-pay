{
  "name": "h4kuna/fio",
  "type": "library",
  "description": "Read JSON file from Fio bank and can send request.",
  "homepage": "https://github.com/h4kuna/fio",
  "license": "MIT",
  "authors": [
    {
      "name": "Milan Matějček",
      "email": "milan.matejcek@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php-64bit": ">=7.1",
    "ext-mbstring": "*",
    "ext-xmlwriter": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^6.0 | ^7.0",
    "nette/safe-stream": "^2.4",
    "nette/utils": "^2.0 | ^3.0"
  },
  "require-dev": {
    "salamium/testinium": "^0.1",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "h4kuna\\Fio\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "h4kuna\\Fio\\": [
        "tests/src/"
      ],
      "h4kuna\\Fio\\Test\\": [
        "tests/data/"
      ]
    }
  }
}
